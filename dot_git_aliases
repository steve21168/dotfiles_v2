alias g='git'
alias gst='g status'
alias gd='g diff'
alias gdc='g diff --cached'
alias gp='g push'
alias gpo='g push origin'
alias gu='g pull'
alias grbi='g rebase -i'
alias ga='g add'
alias gco='g checkout'
alias gcob='g checkout -b'
alias gcbz='g checkout $(g branch | fzf)'
alias gcam='g commit --amend'
alias gc='g commit -m'
alias gbd='g branch -D $(g branch | fzf -m)'
